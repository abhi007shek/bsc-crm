    <webLinks>
        <fullName>Activate</fullName>
        <availability>online</availability>
        <description>Activate Company</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Activate</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)} 

try
{
	// call the webservice to determine the multi-currency status
	var result = sforce.apex.execute(&quot;c2g.CODACompanyWebService&quot;, &quot;getDefaultCurrencyArtifacts&quot;, {} ); 
	// convert result to text and split it into success&gt;&gt;currencyCode&gt;&gt;promptText
	var resulttext = result.toString();
	var rtnval = resulttext.split(&quot;&gt;&gt;&quot;);

	// get the success status and output the messages from the web service
	var success=rtnval[0];
	var singleCurrencyMode=rtnval[1];
	var defaultCurrency=rtnval[2];

	// if its failed, throw the message as an exception
	if(success == &quot;false&quot;)
	{
		// concat the remaining elements in the return value to construct the message
		var msg = &quot;&quot;;
		for (i=3; i&lt;rtnval.length; i++)
		{
			msg += rtnval[i];
		}

		// contruct an error
		var err = new Error();
		err.name = &quot;getDefaultCurrencyArtifacts failed&quot;;
		err.message = msg;
		throw err;
	}

	if(singleCurrencyMode == &apos;true&apos; &amp;&amp; defaultCurrency == &apos;null&apos;)
	{
		defaultCurrency = prompt(rtnval[3]);
		if (typeof(defaultCurrency)==&quot;undefined&quot; || defaultCurrency == null || defaultCurrency.length == 0)
		{
			// contruct an error
			var err = new Error();
			err.name = &quot;isoCode entry cancelled&quot;;
			throw err;
		}
	}

	// call the web service to create the queue
	result = sforce.apex.execute(&quot;c2g.CODACompanyWebService&quot;, &quot;createQueue2&quot;, { companyId:&quot;{!codaCompany__c.Id}&quot;, currencyISOCode:defaultCurrency }); 
	// convert result to text and split it into success&gt;&gt;returnid&gt;&gt;messagetext
	resulttext = result.toString();
	rtnval = resulttext.split(&quot;&gt;&gt;&quot;);

	// get the success status and output the messages from the web service
	success=rtnval[0];

	// if its failed, throw the message as an exception
	if(success == &quot;false&quot;)
	{
		// concat the remaining elements in the return value to construct the message
		var msg = &quot;&quot;;
		for (i=2; i&lt;rtnval.length; i++)
		{
			msg += rtnval[i];
		}

		// contruct an error
		var err = new Error();
		err.name = &quot;getDefaultCurrencyArtifacts failed&quot;;
		err.message = msg;
		throw err;
	}

	// call the web service to activate the company
	result = sforce.apex.execute(&quot;c2g.CODACompanyWebService&quot;, &quot;activateCompany2&quot;, { companyId:&quot;{!codaCompany__c.Id}&quot;, currencyISOCode:defaultCurrency } );

	// convert result to text and split it into success&gt;&gt;returnid&gt;&gt;messagetext
	resulttext = result.toString();
	rtnval = resulttext.split(&quot;&gt;&gt;&quot;);
	
	// get the success status and output the messages from the web service
	success=rtnval[0];

	// concat the remaining elements in the return value to construct the message
	var msg = &quot;&quot;;
	for (i=2; i&lt;rtnval.length; i++)
	{
		msg += rtnval[i];
	}
	alert(msg);
}
catch (err)
{
	if (err.message.length &gt; 0)
	{
		alert(err.message);
	}
}</url>
