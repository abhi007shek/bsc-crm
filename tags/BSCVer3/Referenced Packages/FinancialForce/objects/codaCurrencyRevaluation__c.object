    <webLinks>
        <fullName>Cancel</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Cancel</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)}

// call the webservice to prompt the user to enter cancel reason
var result = sforce.apex.execute(&quot;c2g.CODACurrencyRevaluationWebService&quot;, &quot;canCancelCurrReval&quot;, {currencyRevaluationId:&quot;{!codaCurrencyRevaluation__c.Id}&quot;} );
// convert result to text promptText
var resulttext = result.toString();
var rtnval = resulttext.split(&quot;&gt;&gt;&quot;);

// get the success status and output the messages from the web service
var success=rtnval[0];
var promptMesg=rtnval[1];
var cancelReasonVar=&quot;&quot;;

// if its failed, throw the message as an exception
if(success == &quot;false&quot;)
{
if(rtnval[2]== null || rtnval[2].length == 0)
{
alert(rtnval[1]);
}
else
{
// contruct an error
var err = new Error();
err.name = &quot;canDiscardCurrReval failed&quot;;
err.message = rtnval[2];
throw err;
}

}
else
{
cancelReasonVar= prompt(promptMesg); 

var result = sforce.apex.execute(&quot;c2g.CODACurrencyRevaluationWebService&quot;, &quot;cancelCurrencyRevaluation&quot;, {currencyRevaluationId:&quot;{!codaCurrencyRevaluation__c.Id}&quot;,cancelReason:cancelReasonVar} );

// convert result to text and split it into success&gt;&gt;messagetext
var resulttext = result.toString();
var rtnval = resulttext.split(&quot;&gt;&gt;&quot;);

// get the success status and output the messages from the web service
success=rtnval[0];

// concat the remaining elements in the return value to construct the message
msg = &quot;&quot;;
for (i=1; i&lt;rtnval.length; i++)
msg += rtnval[i];
alert(msg);

if (success == &quot;true&quot;)
{
window.location.reload(true);
}
}</url>
    </webLinks>
